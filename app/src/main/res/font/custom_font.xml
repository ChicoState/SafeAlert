<?xml version="1.0" encoding="utf-8"?>

<font-family xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto">

<!-- regular -->
<font
    android:font="@font/objectSans_Regular"
    android:fontStyle="normal"
    android:fontWeight="400"

    app:font="@font/objectSans_Regular"
    app:fontStyle="normal"
    app:fontWeight="400" />

<!-- italic -->
<font
    android:font="@font/objectSans_Slanted"
    android:fontStyle="italic"
    android:fontWeight="400"

    app:font="@font/objectSans_Slanted"
    app:fontStyle="italic"
    app:fontWeight="400" />

<!-- bold -->
<font
    android:font="@font/objectSans_Heavy"
    android:fontStyle="normal"
    android:fontWeight="700"

    app:font="@font/ObjectSans"
    app:fontStyle="normal"
    app:fontWeight="700" />

<!-- bold italic -->
<font
    android:font="@font/objectSans_HeavySlanted"
    android:fontStyle="normal"
    android:fontWeight="700"

    app:font="@font/objectSans_HeavySlanted"
    app:fontStyle="normal"
    app:fontWeight="700" />
</font-family>